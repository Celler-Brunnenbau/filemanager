    "keywords": [
        "livewire-filemanager",
        "filemanager"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LivewireFilemanager\\Filemanager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Yves Engetschwiler",
