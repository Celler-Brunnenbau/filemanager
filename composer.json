{
    "name": "livewire-filemanager/filemanager",
    "description": "A simple, friendly and practical Livewire filemanager for your applications",
    "homepage": "https://github.com/livewire-filemanager/filemanager",
    "keywords": [
        "livewire-filemanager",
        "filemanager"
    ],
    "authors": [
        {
            "name": "Yves Engetschwiler",
            "email": "info@bee-interactive.ch",
            "role": "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LivewireFilemanager\\Filemanager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireFilemanager\\Filemanager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireFilemanager\\Filemanager\\FilemanagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^3.0",
        "masmerise/livewire-toaster": "^2.0",
        "spatie/laravel-medialibrary": "^10.15",
        "wire-elements/modal": "^2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "orchestra/testbench": "^8.15",
        "phpunit/phpunit": "^10.4"
    }
}
